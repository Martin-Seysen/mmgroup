/// @cond DO_NOT_DOCUMENT 


#define N_OPS_MAP_STD_SUBFRAME %{ShortCocode_NumOps}

static const uint8_t TABLE_IN_MAP_STD_SUBFRAME[11] = {
// %%TABLE ShortCocode_InTable, uint8
